/* 开始改造 */
.page-hd {
  padding: 48rpx 0 60rpx 0;
  background-color:#FFF;
}
.page-hd__txt-title {
  display:block;
  padding-bottom: 26rpx;
  text-align:center;
  line-height:1.3;
  font-size:28rpx;
  color:#B0B3BF;
}
.page-hd__txt-desc {
  padding-top:20rpx;
  color:#9B9B9B;
  font-size:32rpx;
}
.page-hd__line {
  margin:0 auto;
  width:150rpx;
  height:2rpx;
  background-color:#EBEDF5;
}
.page-bd {
  flex:1;

  padding:20rpx 0;
}

.page-section {}

.info-wrap {
  display:flex;
  flex-direction:column;
  align-items:center;
  padding:40rpx 0;
  background-color:#FFF;
  box-sizing:border-box;
}
.info-wrap__txt-title {
  display:block;
  margin-bottom:120rpx;
  text-align:center;
  line-height:1.5;
  font-size:34rpx;
  color:#03081A;
}
.info-wrap__txt {
  display:block;
  text-align:center;
  line-height:1.3;
  font-size:28rpx;
  color:#B0B3BF;
}

.page-ft {
  margin:20rpx 0;
  padding:2rpx 0;
  text-align:center;
}
.page-ft__icon {
  display:inline-block;
  position:relative;
  top:4rpx;
  z-index:1;
  margin-right:12rpx;
  width:24rpx;
  height:24rpx;
  vertical-align:top;
  background-image:url('data:image/svg+xml;base64,');
  background-size:cover;
}
.page-ft__txt {
  display:inline-block;
  vertical-align:top;
  line-height:1.5;
  font-size:24rpx;
  color:#4D94FF;
}

.page-tips {
  margin: 22rpx 0;
  font-size: 28rpx;
  text-align: center;
  color: #999;
}

.button-preview__disabled {
  text-align: center;
  font-size: 28rpx;
  color: #03081A;
}
