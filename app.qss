.container {
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
  min-height: 100vh;
  /* justify-content: space-between; */
  font-size: 32rpx;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.grayPage {
  background-color: #F5F6FA;
}
